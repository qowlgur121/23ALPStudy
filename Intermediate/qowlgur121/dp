#include <iostream>

int d[20];

int main() {
	std::ios::sync_with_stdio(0);
	std::cin.tie(0);
	d[1] = 1;
	d[2] = 2;
	d[3] = 4;
	for (int i = 4; i < 11; i++) {
		d[i] = d[i - 1] + d[i - 2] + d[i - 3];
	}
	int t;
	std::cin >> t;
	while (t--) {
		int n;
		std::cin >> n;
		std::cout << d[n] << '\n';
	}
}
